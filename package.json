{
  "name": "pwa-starter-kit",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors",
    "Moti Matatyahu"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "private": true,
  "scripts": {
    "start": "node ./configuration/server.js",
    "heroku-postbuild": "webpack --config configuration/webpack.prod.js",
    "start2": "webpack-dev-server --config configuration/webpack.dev.js",
    "start:prod": "webpack-dev-server --config configuration/webpack.prod.js",
    "start:poly": "polymer serve",
    "build": "webpack --config configuration/webpack.prod.js",
    "build:poly": "polymer build",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0",
    "@polymer/lit-element": "^0.6.2",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-dialog": "^3.0.1",
    "@polymer/paper-toast": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@vaadin/vaadin-date-picker": "^3.3.2",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "axios": "^0.18.0",
    "jointjs": "^2.2.1",
    "jquery": "^3.3.1",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "save": "^2.3.3"
  },
  "devDependencies": {
    "@babel/preset-env": "7.2.3",
    "axe-core": "3.0.0",
    "babel-loader": "8.0.5",
    "chai": "4.1.2",
    "clean-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.6.0",
    "del": "3.0.0",
    "dotenv-webpack": "1.6.0",
    "express": "^4.16.4",
    "gulp": "4.0.0",
    "gulp-rename": "1.3.0",
    "gulp-replace": "1.0.0",
    "html-webpack-plugin": "3.2.0",
    "mocha": "5.2.0",
    "pixelmatch": "4.0.2",
    "polymer-cli": "1.7.6",
    "polyserve": "0.27.0",
    "prpl-server": "1.2.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "uglifyjs-webpack-plugin": "2.1.1",
    "wct-browser-legacy": "1.0.1",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.0",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1",
    "webpack-serve": "2.0.3",
    "workbox-webpack-plugin": "3.6.3"
  },
  "config": {
    "puppeteer_skip_chromium_download": true
  }
}
